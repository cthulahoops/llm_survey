<!DOCTYPE html>
<html>
    <head>
        <title>LLM Survey</title>
        <link rel="stylesheet" type="text/css" href="style.css">
    </head>
    <body>
        <main>
            <table>
                <tr>
                    <th>Company</th>
                    <th>Model</th>
                    <th>Human-like</th>
                    <th>Cost</th>
                    <th>Consistency</th>
                    <th>Score 1</th>
                    <th>Score 2</th>
                    <th>Score 3</th>
                    <th>Total</th>
                </tr>
                {% for model in models %}
                    <tr>
                        <td>
                            {{ model | model_company }}
                        </td>
                        <td>
                            {{ model | model_link }}
                        </td>
                        {% with similarity = summed_models[model] | similarity(reference_model) %}
                            <td style="--data-value: {{ similarity }}">
                                {{ "%.2f"|format(similarity) }}
                            </td>
                        {% endwith %}
                        <td>
                            ${{ "%.4f"|format(costs[model]) }}
                        </td>
                        {% with consistency = consistencies[model] %}
                            <td style="--data-value: {{ consistency }}">
                                {{ "%.2f"|format(consistency) }}
                            </td>
                        {% endwith %}
                        {% for item in data[model] %}
                        {% with score = item.score %}
                        {% if score is not none %}
                            <td style="--data-value: {{ score / 10 }}">
                                {{ "%.1f"|format(score) }}
                            </td>
                        {% else %}
                            <td></td>
                        {% endif %}
                        {% endwith %}
                        {% endfor %}
                        {% with score = scores[model] %}
                        {% if score is not none %}
                            <td style="--data-value: {{ score / 30 }}">
                                {{ "%.1f"|format(score) }}
                            </td>
                        {% else %}
                            <td></td>
                        {% endif %}
                        {% endwith %}
                    </tr>
                {% endfor %}
            </table>
        </main>
    </body>
</html>
